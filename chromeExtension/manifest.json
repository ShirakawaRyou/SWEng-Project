{
  "manifest_version": 3,
  "name": "Rate Your Resume",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "*://linkedin.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["image/default-avatar.png"],
      "matches": ["*://*.linkedin.com/*", "*://linkedin.com/*"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      
    },
    "default_title": "切换顶部侧边栏"
  },
  "icons": {
    
  }
}